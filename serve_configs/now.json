{
  "name": "vadirn",
  "alias": "vadirn",
  "type": "static",
  "static": {
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "playground/**",
        "destination": "pages/playground.html"
      },
      {
        "source": "/",
        "destination": "pages/home.html"
      }
    ]
  }
}